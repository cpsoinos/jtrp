<div class="fileinput fileinput-new text-center" data-provides="fileinput">

  <div class="preview thumbnail img-raised"></div>
  <span class="btn-file" style="display: inline-block;">
    <div class="thumbnail img-raised">
      <%= image_tag("image_placeholder.jpg") %>
    </div>
    <%= f.cl_image_upload(:initial_photos, multiple: true, return_delete_token: true) %>
  </span>

  <div class="progressbar fileupload-progressbar">
    <div style="width:0%;"></div>
  </div>
  <span class="status"></span>
</div>

<div class="row">
  <div class="col-xs-12 col-md-8 col-md-offset-2">
    <div class="well bs-component">

      <% if @statement.pdf.present? %>
        <iframe src="<%= @statement.pdf_url %>" width="100%" style="border: none;" class="full-height"> </iframe>
      <% else %>
        <%= render 'proposals/header', form_name: "Consignment Sales" %>
        <%= render 'items/items_list', items: @statement.items, headers: statement_headers, context: "consignment_statement" %>
      <% end %>

    </div>
  </div>
</div>
